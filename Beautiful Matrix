#include <bits/stdc++.h>
using namespace std;
int main(){
    int arr[5][5];
    for (int i=0;i<5;i++){
        for (int j=0;j<5;j++){
            cin>>arr[i][j];
        }
    }
    int x,y;
  for (int i=0;i<5;i++){
      for (int j=0;j<5;j++){
          if (arr[i][j]==1){
              x=i;
              y=j;
              break;
          }
      }
  }
  int ans=abs(2-x)+abs(2-y);
  cout<<ans<<endl;
  return 0;
}
https://codeforces.com/problemset/problem/263/A

