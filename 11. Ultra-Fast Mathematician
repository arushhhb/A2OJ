https://codeforces.com/problemset/problem/61/A

#include <bits/stdc++.h>
using namespace std;
int main(){
    string num1,num2;
    cin>>num1>>num2;
    int len=num1.length();
    string result;
    for (int i=0;i<len;i++){
        if (num1[i]!=num2[i]){
            result.push_back('1');
        }
        else {
            result.push_back('0');
        }
    }
    cout<<result<<endl;
    return 0;
}
