https://codeforces.com/problemset/problem/236/A

#include <bits/stdc++.h>
using namespace std;

int main(){
    string s;
    cin>>s;
    int distinct=0;
    for (int i=0;i<s.size();i++){
      bool isSame=false;
      for (int j=i;j<s.size();j++){
        if (s[i]==s[j] && i!=j){
          isSame=true;
          break;
        }
      }
       if(isSame){
         distinct++;
       }
    }
    int ans=s.size()-distinct;
    if (ans%2==0){
      cout<<"CHAT WITH HER!"<<endl;
    }
    else {
      cout<<"IGNORE HIM!"<<endl;
    }
   // cout<<ans<<endl;
    return 0;
}

