https://codeforces.com/problemset/problem/118/A

#include <bits/stdc++.h>
using namespace std;
int main() {
    string s;
    cin>>s;
    string result="";
    for (char c: s){
      c=tolower(c);
      if (c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||c=='y'){
        continue;
      }
      else{ 
        result+='.';
        result+=c;
      }
      
    }
    cout<<result<<endl;
}
